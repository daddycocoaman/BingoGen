{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "Conference redir": {
      "matchCondition": {
        "route": "/conference",
        "methods": ["GET"]
      },
      "backendUri": "https://localhost/card?type=conference"
    },
    "Replystorm redir": {
      "matchCondition": {
        "route": "/replystorm",
        "methods": ["GET"]
      },
      "backendUri": "https://localhost/card?type=replystorm"
    },
    "KJ redir": {
      "matchCondition": {
        "route": "/kjlife",
        "methods": ["GET"]
      },
      "backendUri": "https://localhost/card?type=kjlife"
    },
    "ATT&CK redir": {
      "matchCondition": {
        "route": "/mitreattack",
        "methods": ["GET"]
      },
      "backendUri": "https://localhost/card?type=mitreattack"
    },
    "PresDebate redir": {
      "matchCondition": {
        "route": "/presdebate",
        "methods": ["GET"]
      },
      "backendUri": "https://localhost/card?type=presdebate"
    },
    "Root redir": {
      "matchCondition": {
        "route": "/",
        "methods": ["GET"]
      },
      "backendUri": "https://localhost/index"
    }
  }
}
